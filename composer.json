{
  "name": "yoelpc4/ea-wp-menu",
  "description": "Custom preset for Laravel frontend scaffolding.",
  "keywords": [
    "laravel",
    "preset",
    "scaffolding",
    "bootstrap",
    "jquery"
  ],
  "homepage": "https://github.com/yoelpc4/ea-wp-menu",
  "license": "MIT",
  "authors": [
    {
      "name": "Yoel Peter Chandra",
      "email": "yoelpc4@gmail.com",
      "role": "Developer"
    }
  ],
  "require": {
    "illuminate/console": "5.8.*",
    "illuminate/support": "5.8.*"
  },
  "suggest": {
    "laravelcollective/html": "Plugin to build form input (v5.8.1)",
    "realrashid/sweet-alert": "Plugin to build alert",
    "yajra/laravel-datatables-oracle": "Plugin to build datatables",
    "yajra/laravel-datatables-buttons": "Plugin to build datatables button",
    "yoelpc4/laravel-attachment": "Plugin to build attachment manager (v1.0.0)"
  },
  "autoload": {
    "psr-4": {
      "Yoelpc4\\EAWpMenu\\": "src/"
    },
    "files": [
      "src/helpers.php"
    ]
  },
  "extra": {
    "laravel": {
      "providers": [
        "Yoelpc4\\EAWpMenu\\EAWpMenuServiceProvider"
      ]
    }
  },
  "config": {
    "sort-packages": true
  },
  "minimum-stability": "stable",
  "prefer-stable": true
}
